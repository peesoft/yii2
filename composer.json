{
    "name": "peesoft/yii-dev",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "replace": {
        "yiisoft/yii2": "self.version"
    },
    "require": {
        "php": ">=7.1",
        "ext-mbstring": "*",
        "ext-ctype": "*",
        "lib-pcre": "*",
        "yiisoft/yii2-composer": "~2.0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.34",
        "cebe/indent": "~1.0.2",
        "friendsofphp/php-cs-fixer": "~2.2.3"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "yii\\": "framework/"
        }
    },
    "config": {
        "platform": {"php": "7.1"}
    },
    "bin": [
        "framework/yii"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
